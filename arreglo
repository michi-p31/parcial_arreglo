# include<iostream>
#include<conio.h>
using namespace std;
int main(){
	int a,b;
int numeros[30]={2,4,6,8,10,12,14,16,18,19,20,21,22,24,26,28,30,32,34,36,38,40,42,44,46,48,50,52,54,56},
;
cout<<"Que numero para que desea consultar del arreglo ";
cin>>aux;
for(a=0;a<5;a++){
		
		for (b=0;b<5;b++){
			
			if(numeros[b] > numeros[b+1]){
				
				aux=numeros[b];
				numeros[b]=numeros[b+1];
				numeros[b+1]=aux;
				
			}
if (aux){
	cout<<"Se encontro en el arreglo";
	
}else{
	cout<<"Numero";
}


}

